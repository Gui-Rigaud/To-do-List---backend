generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Membro {
  id       String    @id @default(uuid())
  email    String @db.VarChar(255)
  password String @db.VarChar(255)
  nome     String @db.VarChar(255)

  @@map("membros")
}